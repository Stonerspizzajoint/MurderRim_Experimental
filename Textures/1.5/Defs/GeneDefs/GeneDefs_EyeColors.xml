<Defs>


  	<VREAndroids.AndroidGeneDef Name="MD_DisplayColorBase" Abstract="True">
		<displayCategory>VREA_Hardware</displayCategory>
		<canGenerateInGeneSet>false</canGenerateInGeneSet>
		<displayOrderInCategory>10006</displayOrderInCategory>
		<isCoreComponent>false</isCoreComponent>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathXenogenes>UI/Icons/Genes/GeneBackground_Hardware</backgroundPathXenogenes>
				<backgroundPathEndogenes>UI/Icons/Genes/GeneBackground_Hardware</backgroundPathEndogenes>
			</li>
			<li Class="WorkerDronesMod.GenePrerequisitesExtension">
				<prerequisiteGeneDefNames>
					<li>MD_DisplayEyes</li>
					<li>MD_DisplayEye</li>
					<li>MD_DisplayEyes_DD</li>
				</prerequisiteGeneDefNames>
			</li>
		</modExtensions>
	</VREAndroids.AndroidGeneDef>
	
	<VREAndroids.AndroidGeneDef ParentName="MD_DisplayColorBase">
    	<defName>MD_DisplayColor_White</defName>
    	<label>white display</label>
    	<labelShortAdj>pale</labelShortAdj>
    	<description>Carriers of this gene have white eyes and lights.</description>
    	<skinColorOverride>(255, 255, 255)</skinColorOverride>
	    <iconPath>Pawn/Display/DisplayEyesBase/DisplayEyes1</iconPath>
    	<exclusionTags>
      		<li>EyeColor</li>
	  		<li>SkinColorOverride</li>
    	</exclusionTags>
		<biostatCpx>1</biostatCpx>
    	<randomChosen>true</randomChosen>
    	<randomBrightnessFactor>1</randomBrightnessFactor>
  	</VREAndroids.AndroidGeneDef>

